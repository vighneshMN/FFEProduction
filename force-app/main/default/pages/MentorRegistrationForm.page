                    </div>
                    <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label">Are you Self-Employed?</label>
                                <input type="checkbox" ng-model="SelfEmployed" />
                                <div class="form-group focused" ng-show="SelfEmployed">
                                    <!--label class="form-control-label" >Employer Name</label>
                                    <input type="text"  class="form-control form-control-alternative" placeholder="Employer Name" value="{{mentorDetail.Employer_Name__c}}" ng-model="mentorDetail.Employer_Name__c" /-->
                                    <label class="form-control-label" >Job Title / Professional Designation</label>
                                    <input type="text"  class="form-control form-control-alternative" placeholder="Designation" ng-model="mentorDetail.Self_Empoyee_Designation__c" maxlength="200"/>
                                </div>
                            </div>
                        </div>
                        <!--div class="col-lg-6">
                            <div class="form-group">
                                <label class="form-control-label">Organizational Sector</label><span style="color:red">*</span>
                                <input type="text" class="form-control form-control-alternative" placeholder="Organizational Sector" value="" ng-model="mentorDetail.Organizational_Sector__c" maxlength="200"/>
                            </div>
                        </div-->
                    </div>
                    <hr class="my-4" />
                    <h4 class="heading-small text-muted mb-4">Educational information</h4>
                    <div class="row pl-lg-4">
                    <div class="col-lg-6">
                        <div class="form-group focused">
                            <label class="form-control-label" >College Name</label>
                            <input type="text" class="form-control form-control-alternative" placeholder="College name" value="{{mentorDetail.Mentor_College_Name__c}}" ng-model="mentorDetail.Mentor_College_Name__c"  maxlength="200"/>
                        </div>
                    </div>
                    <div class="col-lg-6">
                    </div>
                    </div>
                    <div class="row pl-lg-4">
                    <div class="col-lg-6">
                        <div class="form-group focused">
                            <label class="form-control-label">Highest Educational Qualification</label>
                            <select class="form-control" style="width:100%;" ng-model="mentorDetail.Highest_Educational_Qualification__c" ng-change="highest_Education(mentorDetail.Highest_Educational_Qualification__c)">
                                <option ng-repeat="x in HighestQualification" >
                                {{x}}
                                </option>
                            </select>
                        </div>
                    </div>
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label" >Branch of Study</label><span style="color:red">*</span>
                                <select class="form-control" style="width:100%;" ng-model="mentorDetail.Branch_Areas_of_Practice_Study__c" ng-change="branchOfPractice(mentorDetail.Branch_Areas_of_Practice_Study__c)">
                                    <option ng-repeat="x in branchOfStudy" >
                                    {{x}}
                                    </option>
                                </select>
                            </div>
                        </div>
                        </div>
                       
                        <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div  class="form-group focused"  ng-if="showHighestQualificationInput">
                                <label class="form-control-label" >Highest Qualification</label>
                                <input type="text" class="form-control form-control-alternative" value="{{mentorDetail.Other_Highest_Educational_Qualification__c}}" ng-model="mentorDetail.Other_Highest_Educational_Qualification__c" maxlength="200"/>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div  class="form-group focused"  ng-show="showBranchOfPractice">
                                <label class="form-control-label" >Branch of study</label>
                                <input type="text" class="form-control form-control-alternative" value="{{mentorDetail.Mentor_Branch_Of_Study__c}}" ng-model="mentorDetail.Mentor_Branch_Of_Study__c" maxlength="200"/>
                            </div>
                        </div>
                        </div>
                    <hr class="my-4"/>
                    <!-- Address -->
                    <h4 class="heading-small text-muted mb-4">Contact information</h4>
                    <div class="pl-lg-6">
                       
                        <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label" for="input-city">City</label>
                                <input type="text" id="input-city" class="form-control form-control-alternative" placeholder="City" value="{{mentorDetail.MailingCity}}" ng-model="mentorDetail.MailingCity" maxlength="37"/>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label" for="input-country">Country</label>
                                <input type="text" id="input-country" class="form-control form-control-alternative" placeholder="Country" value="{{mentorDetail.MailingCountry}}" ng-model="mentorDetail.MailingCountry" maxlength="75"/>
                            </div>
                        </div>
                        </div>
                    </div>
                    <hr class="my-4" />
                    <h4 class="heading-small text-muted mb-4">About me</h4>
                    <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label">Are you an FFE Alumni?</label>
                                Yes<input type="checkbox" placeholder="is alumni" ng-model="isAlumuni" ng-click="checkisAlumuni('isAlumuni',isAlumuni)"/>/&nbsp;No
                                <input type="checkbox" placeholder="is alumni" ng-model="isNotAlumuni" ng-click="checkisAlumuni('isNotAlumuni',isNotAlumuni)" />
                            </div>
                        </div>
                        
                        <div class="col-lg-6">
                            <div class="form-group focused" ng-if="mentorDetail.Is_Active_Alumni__c">
                                <label class="form-control-label">Year Of Graduation</label><span style="color:red">*</span>
                                <div >
                                   <input type="text" class="form-control form-control-alternative" placeholder="year of graduation"  ng-model="mentorDetail.DN_Year_Of_Graduation__c" maxlength="75"/> 
                                </div>
                            </div>
                        </div>
                       
                    </div>
                    <div class="row pl-lg-4">
                    <div class="col-lg-6">
                        <div class="form-group focused">
                            <label>Have you been a mentor before?</label>
                            <input type="checkbox"   ng-model="mentorDetail.Been_a_mentor_before__c"/>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group focused" ng-show="mentorDetail.Been_a_mentor_before__c">
                            <label>Please provide us some more information regarding the mentorship, and if you are continuing to mentor:
                            </label>
                            <textarea rows="3" class="form-control form-control-alternative" ng-model="mentorDetail.More_info_on_mentoring__c" placeholder="A few words on mentorship ..." maxlength="30000">A beautiful Dashboard for Bootstrap 4. It is Free and Open Source.</textarea>
                        </div>
                    </div>
                    </div>
                     
                    <div class="row pl-lg-4">
                    <div class="col-lg-6">
                        <div class="form-group focused">
                            <label>Please tell us a bit more about yourself to help us pair you with an appropriate mentee (Hobbies, interests,etc)</label>
                            <textarea rows="2" class="form-control form-control-alternative" ng-model="mentorDetail.hobbies_and_interests__c" placeholder="A few words about you ..." maxlength="900">A beautiful Dashboard for Bootstrap 4. It is Free and Open Source.</textarea>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group focused">
                            <label>Please tell us why you would like to mentor FFE scholars</label>
                            <textarea rows="3" class="form-control form-control-alternative" ng-model="mentorDetail.TYS_How_would_you_help_other_Students__c" placeholder="A few words about you would like to mentor FFE scholars ..." maxlength="900">A beautiful Dashboard for Bootstrap 4. It is Free and Open Source.</textarea>
                        </div>
                    </div>
                    </div>
                    <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label" for="input-city">Are you prepared (and have capacity) to commit a minimum of 2 hours per month to this 4-6 month programme (per mentee)?</label>
                                <input type="checkbox"  value="" ng-model="mentorDetail.prepared_to_commit_2hr_per_mentee__c"/>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label" for="input-country">Your preferred time to conduct the mentoring session:</label><span style="color:red">*</span>
                                <select class="form-control" style="width:100%;" ng-model="mentorDetail.Preferred_time_to_conduct_session__c" >
                                    <option ng-repeat="x in timeToConductSession" >
                                    {{x}}
                                    </option>
                                </select>
                            </div>
                        </div>
                    </div>
                   
                    <hr class="my-4" />
                    <div class="row pl-lg-4">
                        <div class="col-lg">
                            <div class="form-group focused">
                                <label>General comments that you feel are important for us to know</label>
                                <textarea rows="3" class="form-control form-control-alternative" ng-model="mentorDetail.General_comments_on_us__c" placeholder="A few words about us ..." maxlength="900">A beautiful Dashboard for Bootstrap 4. It is Free and Open Source.</textarea>
                            </div>
                            <div align="center"> 
                                <button class="download btn btn-lg buttons" ng-click="saveMentorDetails()">Save</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            </div>
        
    </body>
    <script>
        debugger;
        angular.module('number', [])
    .directive('number', function () {
        debugger;
        return {
            require: 'ngModel',
            restrict: 'A',
            link: function (scope, element, attrs, ctrl) {
                ctrl.$parsers.push(function (input) {
                    debugger;
                    if (input == undefined) return ''
                    var inputNumber = input.toString().replace(/[^0-9]/g, '');
                    debugger;
                    if (inputNumber != input) {
                        ctrl.$setViewValue(inputNumber);
                        ctrl.$render();
                    }
                    return inputNumber;
                });
            }
        };
    });
        var app = angular.module('mentorRegApp', ['number']);
        app.controller('mentorRegistrationCtrl', function ($scope) {
            $scope.showSpinner = false; 
            $scope.isAlumuni = false; 
            $scope.isNotAlumuni = true;
            var pageLoadData = JSON.parse('{!pageLoadInstanceString}');
            $scope.genderPicklist = pageLoadData.picklistValues.mentorGender;
            $scope.spokenLangPicklist = pageLoadData.picklistValues.spokenLang;
            $scope.branchPicklist = pageLoadData.picklistValues.branch;
            $scope.mentoringNeedsPicklist = pageLoadData.picklistValues.mentoringNeeds;
            $scope.currentlyEmployedPickList = pageLoadData.picklistValues.AreYouCurrentlyEmployed;
            $scope.HighestQualification = pageLoadData.picklistValues.HighestEducationalQualification;
            $scope.branchOfStudy = pageLoadData.picklistValues.BranchofPracticeStudy;
            $scope.timeToConductSession = pageLoadData.picklistValues.PreferredTimeToConductSession;
            $scope.CorporateName = pageLoadData.picklistValues.CorporateName;
            $scope.selected;
            $scope.obj = {
                menteeForm: false,
                menteeLogin: true
            };
           
            $scope.spokenLangList = [];
            $scope.selectedSpokenLang = [];
            $scope.idToVerify;
            $scope.emailToverify;
