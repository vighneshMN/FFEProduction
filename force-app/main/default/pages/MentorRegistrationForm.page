<apex:page sidebar="false" showHeader="false" standardStylesheets="false" controller="MenteePortal_Controller" action="{!checkUserAuth}">
    <head>
        <c:MentorshipScripts />
        <script src="ng-pattern-restrict.js">
        </script>
        <style>
            .loadingBackground {
                position: fixed;
                height: 100%;
                width: 100%;
                background: #F6F6F6;
                z-index: 11111;
                opacity: 0.7;
                top: 0px;
                left: 0;
                bottom: 0;
                right: 0;
            }

            .loading {
                top: 50%;
                left: 50%;
                margin: -50px 0px 0px -50px;
                z-index: 111111;
                position: fixed;
            }
            .card-profile-image img {
                box-shadow: 0 0 2rem 0 rgba(136, 152, 170, .15) !important;
            }

            .card-profile-image {
                position: relative;
            }

            .card-profile-image img {
                position: absolute;
                left: 50%;
                max-width: 70px;
                transition: all .15s ease;
                transform: translate(-50%, -30%);
                border-radius: .375rem;
            }

            .card-profile-image img:hover {
                transform: translate(-50%, -33%);
            }

            .rounded-circle {
                border-radius: 50% !important;
            }

            .profileDetailCard {
                box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.2);
                transition: 0.3s;
                Fwidth: 73%;
            }

            .card {
                box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.2);
                transition: 0.3s;
                width: 100%;
            }

            .card:hover {
                box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
            }

            .container {
                padding: 2px 16px;
                /* margin-left: 20%;  */
                width: 90%;
            }

            .cardContainer {
                padding: 2px 16px;
                margin-left: 20%;
            }

           

           .buttons {
                border-radius:3px;
                
                color:#000000;
                border: 1px solid #b1976b;
                text-transform:uppercase;
                background: #f4a52e;
            }
            .buttons:hover,.cancelButton:hover{
                box-shadow: rgb(127 127 182 / 25%) 0px 0px 16px;
            }
            .buttons:hover {
            opacity: 0.7;
            color:#ffffff;
            background-color:black !important;
            }
            input:hover {
                background-color: #d2d2d2 !important;
            }

            .card {
                box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
                transition: 0.3s;
                width: 60%;
                margin-left: 23%;
                margin-top: 6% !important;
            }

            .card:hover {
                box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
            }

            .topnav .login-container {
                float: right;
            }

            .container {
                padding: 16px;
            }

            .topnav input[type=text] {
                padding: 6px;
                margin-top: 8px;
                font-size: 17px;
                border: none;
                width: 120px;
            }

            .topnav .login-container button {
                float: right;
                padding: 6px 10px;
                margin-top: 8px;
                margin-right: 16px;
                background-color: #555;
                color: white;
                font-size: 17px;
                border: none;
                cursor: pointer;
            }

            .topnav .login-container button:hover {
                background-color: green;
            }

            @media screen and (max-width: 600px) {
                
                .topnav .login-container {
                    float: none;
                }
                .topnav a,
                .topnav input[type=text],
                .topnav .login-container button {
                    float: none;
                    display: block;
                    text-align: left;
                    width: 100%;
                    margin: 0;
                    padding: 14px;
                }
            }
            .download {
                background-color: #F39C12;
                color: white;
                padding: 5px 20px;
                cursor: pointer;
            }

            .download:hover {
                opacity: 0.7;
            }
            @media screen and (min-width:851px){
            .card{
                    margin-top:10% !important;
                }
            }
            @media screen and (max-width:850px){
                .card{
                    margin-top:15% !important;
                }
            }
            .fonts{
                text-align:center;
                font-weight: 700;
            }
            @media screen and (max-width:600px){
                
                .navbar-brand{
                    width:100%;
                    padding:unset;
                }
           
                .logo{
                    width:100%;
                    height:75px;
                    max-width:400px;
                }
                .card{
                width: 94% !important;
                margin-left:3% !important;
                margin-top:110px !important;
                }
            }
            
            
            @media screen and (max-width:450px){
                .fonts{
                    font-size:20px;
                    font-weight:600;
                }
            }
            @media screen and (max-width:400px){
                nav.navbar.fixed-top.navbar-light.bg-light.navbar-expand-lg{
                padding-left:unset;
                padding-right:unset;
                }
            }
            html,body{
                    background-color: #e5e7eb;
            }
        </style>
    </head>
    <body ng-app="mentorRegApp" ng-controller="mentorRegistrationCtrl">
        <div class="loadingSection" id="spinner" ng-if="showSpinner">
            <div class="loadingBackground"></div>
            <div>
                <apex:image url="{!$Resource.loading}" styleClass="loading" />
            </div>
        </div>
        <nav class="navbar fixed-top navbar-light bg-light navbar-expand-lg">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">
                    <apex:image url="{!$Resource.FFEbanner}" width="50%" height="50%" styleClass="logo" />
                </a>

            </div>
        </nav>
            <div class="card">
            <div class="card-header" style="background-color: #2e3192;color: #ffffff;">
                <h3 style="" class="fonts">
                    Mentor Registration Form
                </h3>
            </div>
            <div class="card-body">
                <div class="">
                    <h5 style="text-align:justify;color:red;font-size:1rem;font-weight: 400;">
                        {!$Label.Mentor_name_alert_banner}
                    </h5>
                </div>
                <form style="">
                    <h4 class="heading-small text-muted mb-4">User information</h4>
                    <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label">First Name</label><span style="color:red">*</span>
                                <input type="text" class="form-control form-control-alternative" placeholder="first name" value="{{mentorDetail.Firstname}}" ng-model="mentorDetail.FirstName" maxlength="35"/>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label">Last Name</label><span style="color:red">*</span>
                                <input type="text" class="form-control form-control-alternative" placeholder="last name" value="{{mentorDetail.Lastname}}" ng-model="mentorDetail.LastName" maxlength="75"/>
                            </div>
                        </div>
                       
                    </div>
                    <div class="row pl-lg-4">
                        <div class="col-lg-6">
                            <div class="form-group focused">
                                <label class="form-control-label">Contact Number</label><span style="color:red">*</span>
                                <input type="text" class="form-control form-control-alternative" placeholder="Phone number" value="{{mentorDetail.Phone}}" ng-model="mentorDetail.Phone" maxlength="11"/>
