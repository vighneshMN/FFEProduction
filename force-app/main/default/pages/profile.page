    <script>
    window.onload=function(){
        var i;
        var x = document.getElementsByClassName("city");
        for (i = 0; i < x.length; i++) {
            if(i==0){
                x[i].style.display = "block";
            }
            else{
                x[i].style.display = "none";
            }
        }
    }
    </script>
    <body >
    <div class="headermenuSelection headersHistory" ng-init="getListofFiles();">
        
        <ul style="width:54%;float:left;">
            <li ng-click="switch(0,0)" ng-class="{activetabs: 0 == selectedtabs}" onclick="openCity('Basic')" ><a  ng-click="moveToTop();">Profile Details</a></li>
            <li  ng-click="switch(2,2);getListofFiles();" ng-class="{activetabs: 2 == selectedtabs}" onclick="openCity('course')" ><a ng-click="moveToTop();">Applications</a></li>
            <li  ng-click="switch(3,3);" ng-if="!IsPrelimApplication" ng-class="{activetabs: 3 == selectedtabs}" onclick="openCity('tellYourStory')" ><a ng-click="moveToTop();">Tell Your Story</a></li>
        </ul>
        
        <div style="width:40%;float: left;padding: 9px 0;">
            <a ng-show="IsPrelimApplication" href="/application/scholar_profile?ScholarId={{studendId}}" target="_blank" style="float: left;/* position: relative; *//* top: 7px; *//* font-size: 11pt; *//* left: -299px; */color: rgb(66, 92, 170);/* text-decoration: underline; */cursor: pointer;width: 30%;text-align: right;">Generate Profile as Pdf</a>
            <!-- <a ng-show="IsPrelimApplication" href="{!$Setup.System_Details__c.Generate_Pledge_Parent_Declaration__c}" target="_blank" style="float: left;position: relative;/* top:7px; *//* font-size: 11pt; *//* margin-left: 10px; */text-align: left;color: #425caa;/* text-decoration: underline; */cursor: pointer;width: 61%;text-indent: 1em;">Generate the Pledge and Parent Declaration</a> -->
        </div>
    </div>
    <ng-form name="basicInfo" novalidate="true" class="switchDiv">
        
        <div id="Basic" class="city switchPages" ng-init="tabs();">
            
            <div class="buttonDiv" align="center" style="margin-bottom:0px;">
                <input type="button" value="Edit" class="buttons"  ng-click="edit();" ng-if="editFileds"/>
                <input type="button" value="Save" class="buttons"  ng-click="save();" ng-if="!editFileds"/>
                <input type="button" value="Cancel" class="buttons cancelButton"  ng-click="cancelChanges();" ng-if="!editFileds"/>
            </div>
            <div class="formDiv">
                <h4 class="formheadings" align="center">
                    Basic Information 
                </h4>
                <div class="row">
                    <h4 class="tagnames" >
                        First Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="fName" ng-model="student.FirstName" text="student.FirstName"  class="inputfileds"  required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.fName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.fName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Last Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userLastName" ng-model="student.LastName" text="student.LastName"  class="inputfileds rightrow" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userLastName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userLastName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Gender:<span style="color:#ff0000">*</span>
                    </h4>
                    
                     <md-input-container>
                        <!--label>Gender</label-->
                        <md-select class="inputfiledsSelect" name="gender" style="color:black;"  ng-model="student.Gender__c" required='true' ng-disabled="editFileds">
                            <md-option  value="Male" >Male</md-option>
                            <md-option  value='Female'>Female</md-option>
                            <md-option  value='Other'>Other</md-option>
                        </md-select>
                    </md-input-container>
                    <span ng-show="formInvalid && basicInfo.gender.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.gender.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Email:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userEmail" ng-model="student.Email"  class="inputfileds rightrow" ng-change="valid(student.Email)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userEmail.$invalid" class="error">This is required.</span>
                    <span class="error" ng-show="(!(basicInfo.userEmail.$invalid) && invalidEmail)">Invalid email address</span>
                    <span ng-show="!(formInvalid && basicInfo.userEmail.$invalid) && !(!(basicInfo.userEmail.$invalid) && invalidEmail) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" >
                    <h4 class="tagnames" >
                        Date Of Birth:<span style="color:#ff0000">*</span>
                    </h4>
                    <md-datepicker style="min-height: 30px;width:-webkit-fill-available;margin-right:0%;width:-moz-available;" class="inputfiledsSelect" type="date" ng-model="student.Birthdate" required="true" name="userDob" id="mydate" ng-disabled="editFileds" md-max-date='todate'></md-datepicker>
                    <span ng-show="formInvalid && basicInfo.userDob.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userDob.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                
                <div class="row" >
                    <h4 class="tagnames" >
                        Aadhaar No: <span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" ng-if="scholarshipGrantedAtLeastOnce || student.KYC_Verified__c" name="userAadhar" ng-model="student.Aadhar_number__c"  class="inputfileds rightrow"  ng-readonly='true'  />
                    <input type="text" ng-if="!scholarshipGrantedAtLeastOnce && !student.KYC_Verified__c" name="userAadhar" ng-model="student.Aadhar_number__c" number="student.Aadhar_number__c"  class="inputfileds"   ng-readonly="editFileds || scholarshipGrantedAtLeastOnce || student.KYC_Verified__c"  required='true' maxlength="12" minlength="12"/> 
                    <span ng-show="formInvalid && basicInfo.userAadhar.$invalid && !student.KYC_Verified__c && !scholarshipGrantedAtLeastOnce" class="error">&nbsp;Enter your 12 digit Aadhaar number.</span>
                </div>
                
                
                <div class="row">
                    <h4 class="tagnames" >
                        Mobile No:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userMobile" ng-model="student.MobilePhone" number="student.MobilePhone"  class="inputfileds"  required='true' maxlength="11" minlength="10" ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userMobile.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userMobile.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        WhatsApp No:
                    </h4>
                    <input type="text" name="userPhone" ng-model="student.Phone" number="student.Phone" class="inputfileds rightrow"  maxlength="11" minlength="6"  ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="(formInvalid && basicInfo.userMobile.$error.minlength || formInvalid && basicInfo.userMobile.$error.maxlength )" class="error">Phone Number length should be between 6-11 digits</span>
                </div>
                <div class="row" ng-if='editFileds'>
                    <h4 class="tagnames" >
                        Student Unique Id:<span style="color:#ff0000"></span>
                    </h4>
                    <input type="text" name="id" ng-model="student.FFE_ID__c"  class="inputfileds" ng-readonly="true"/>              
                </div>
                <div ng-if="editFileds">
                    <div class="row" ng-if="(student.Facilitator_Name__r.Name!=undefined && student.Facilitator_Name__r.Name!='')">
                        <h4 class="tagnames" >
                            Assigned Facilitator Name:
                        </h4>
                        <input type="text" name="Facilitator_Name__c" ng-model="student.Facilitator_Name__r.Name" text="student.Facilitator_Name__r.Name"  class="inputfileds readInput"  readonly='true'/>
                    </div>
                    
                    <div class="row" ng-if='editFileds'>
                        <h4 class="tagnames" >
                            Total Training Point(s) Earned: <span style="color:#ff0000"></span>
                        </h4>
                        <input type="text" name="id" ng-model="student.Total_Point_s__c"  class="inputfileds rightrow" ng-readonly="true"/>              
                    </div>
                    
                    <div class="row" ng-if="(student.Refered_by_Facilitator_Name__c!=undefined && student.Refered_by_Facilitator_Name__c!='')">
                        <h4 class="tagnames" >
                            Facilitator Name:
                        </h4>
                        <input type="text" name="Refered_by_Facilitator_Name__c" ng-model="student.Refered_by_Facilitator_Name__c" text="student.Refered_by_Facilitator_Name__c"  class="inputfileds readInput"  readonly='true'/>
                    </div>
                    <div class="row" ng-if="(student.Refered_by_Facilitator_Id__c!=undefined && student.Refered_by_Facilitator_Id__c!='')">
                        <h4 class="tagnames" >
                            Facilitator Id:
                        </h4>
                        <input type="text" name="Refered_by_Facilitator_Id__c" ng-model="student.Refered_by_Facilitator_Id__c"  class="inputfileds readInput" onpaste="return isAlphaNumber(event)" onkeypress="return isAlphaNumber(event)" readonly='true'/>
                    </div>
                </div>
            </div>
            <div class="formDiv" ng-if="donationDetailList.length >0">
                <h4 class="formheadings" align="center">
                    Donor Information 
                </h4>
                                    <table class="historyTable fullscreen" style="margin-top: 15px;margin-bottom:15px;">
                        <thead>
                            <tr class="rowsHistory" ng-if="namedSchlrshp != ''">
                                <th colspan="2">{{namedSchlrshp}}</th>
                            </tr>
                            <tr class="rowsHistory thRow">
                                <th style="color:#fff">Year</th>
                                <th style="color:#fff">Donor Name</th>
                                <!--th style="color:#fff">Named Scholarship</th-->
                               
                            </tr>
                        </thead>
                        <tr align="center" class="rowsHistory" ng-repeat="donation in donationDetailList">
                            <td>{{donation.year}}</td>
                            <td>{{donation.donorName}}</td>
                            <!--td>{{donation.namedSchlrshp}}</td-->                          
                        </tr> 
                    </table>
                <md-card class="mobac" style="box-shadow:none;">
                    <md-card-title><strong style="width:-webkit-fill-available;">Donation Details</strong></md-card-title>
                    <md-card-content ng-repeat="donation in donationDetailList">
                        <md-card-title ><strong >Year :</strong>  &nbsp; &nbsp;{{donation.year}}</md-card-title>
                        <md-card-title ><strong >Donor Name : </strong>  &nbsp;&nbsp; {{donation.donorName}}</md-card-title>
                        <md-card-title ><strong >Named Scholarship : </strong>  &nbsp;&nbsp; {{donation.namedSchlrshp}}</md-card-title>
                    </md-card-content>
                </md-card>
            </div>
            <div class="formDiv">
                <h4 class="formheadings" align="center">
                    Bank Information    
                </h4>
                <div class="row">
                    <h4 class="tagnames" >
                        Bank Account Holder's Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="acName" ng-model="student.Bank_Account_Holder_s_Name__c" text="student.Bank_Account_Holder_s_Name__c"  class="inputfileds"  required='true' ng-readonly="editFileds || disableBankDetails" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.acName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.acName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Full Bank Account No. (With all digits):<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="acNum" ng-model="student.Full_Bank_Account_No_With_all_digits__c" number="student.Full_Bank_Account_No_With_all_digits__c"  class="inputfileds rightrow"  required='true' ng-readonly="editFileds || disableBankDetails" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.acNum.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.acNum.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                
                
                <div class="row">
                    <h4 class="formTags" >
                        11 characters IFSC Code of the Branch:<span style="color:#ff0000" >*</span>
                    </h4>
                    <input type="text" onpaste="return isAlphaNumber(event)" onkeypress="return isAlphaNumber(event)" name="ifscCode" class="inputfileds"  ng-required="true"  ng-model="student.X11_character_IFSC_Code_of_the_Branch__c" maxlength='11' minlength='11' ng-pattern="/^[a-zA-Z0-9]*$/" ng-readonly="editFileds || disableBankDetails" ng-class="{readInput: editFileds}" ng-change="ifscChanged()"/>
                    <span ng-show="!(formInvalid && basicInfo.ifscCode.$invalid)  && showSpace" class="error">&nbsp;</span>
                    <span ng-show="formInvalid && basicInfo.ifscCode.$invalid" class="error">This is required.</span>
                    <span class="error">&nbsp;</span>
                    <span class="error" ng-if="ifscCodeValid"  style="color: rgb(10, 45, 94);    font-weight: bold;"> {{ifscCodeMessage}}</span>
                    <span class="error" ng-if="ifscCodeValid">&nbsp;</span>
                    
                    <span class="error" ng-show="(!basicInfo.ifscCode.$invalid && basicInfo.ifscCode.$error.minlength)">Invalid PAN details</span>
                    <span class="error" ng-show="!(!basicInfo.ifscCode.$invalid && basicInfo.ifscCode.$error.minlength)  && showSpace">&nbsp;</span>
                    
