    <script>
    var isLogin = '{!$CurrentPage.parameters.login}';
    window.onload = function(){
        var a = document.getElementById('googleUrlId');
        a.href = "https://accounts.google.com/AccountChooser?continue=https://accounts.google.com/o/oauth2/auth?redirect_uri="+"{!$Setup.Google_API_Details__c.Mentor_Mentee_redirect_uri__c}"+"%26prompt%3Dconsent%26response_type%3Dcode%26client_id="+"{!$Setup.Google_API_Details__c.Client_Id__c}"+"%26scope%3Dhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/plus.me%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.profile%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.profile%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/plus.me%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.profile%2Bhttps://www.googleapis.com/auth/plus.login%2Bhttps://www.googleapis.com/auth/plus.me%2Bhttps://www.googleapis.com/auth/userinfo.email%2Bhttps://www.googleapis.com/auth/userinfo.profile%2Bhttps://www.googleapis.com/auth/userinfo.email%26access_type%3Doffline%26from_login%3D1%26as%3D-270badd61a3e150b&btmpl=authsub&scc=1&oauth=1";
        
        var fb  = document.getElementById('facebookUrlId');
        fb.href = "https://www.facebook.com/v2.8/dialog/oauth?client_id="+"{!$Setup.Facebook_API_Details__c.App_Id__c}"+"&response_type=code&redirect_uri="+"{!$Setup.Facebook_API_Details__c.Mentorship_Site_URL__c}"+"&scope=public_profile,email&&auth_type=rerequest";
    }
    var app = angular.module('mentorshipApp', []);
    app.controller('mentorshipCtrl', function($scope) {
        debugger;
        $scope.showSpinner = false;
        $scope.showLoginRegister = true;
        $scope.showMenteeregistration = false;
        $scope.showMentorMenteeSignIn = true;
        $scope.showMentorRegistration = false;
        $scope.showforgotPassword = false;
        
        $scope.mentorSignin = function(){
            if($scope.MentorName == undefined || $scope.Mentorpswrd == undefined) {
                
                return;
            }
            
            $scope.showSpinner = true;
            $scope.Mentorpswrd = $scope.Mentorpswrd.replaceAll(/\s+/g,'');
            MSigninHelper.mentorLogin($scope.MentorName, $scope.Mentorpswrd, function(result, event) {
                if(event.status){
                    if(result != null && result != undefined){
                        if(result.length == 23){
                            let mainURL = window.location.origin+'/mentorship';
                            window.location.replace(mainURL + "/"+"mentorPortal?id="+result);  
                            /*  Swal.fire(
                        '',
                        'Temporary URL sent successfully to your registered email!',
                        'success'
                    )  */
                        }else if(result == 'wpassword'){
                            Swal.fire({
                                icon: 'error',
                                title: '',
                                text: 'The Email/Password you entered is incorrect. Please try again',
                            })
                        }else if(result =="nomentor"){
                            Swal.fire({
                                icon: 'error',
                                title: '',
                                text: 'Please ensure you are signing in with registered Email/Password',
                            })
                            
                        }
                    }
                        
                    }
                    else if(event.type==='exception'){
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }
                    $scope.showSpinner = false;
                    $scope.$apply();
                });
            }
            
            $scope.showRegisterForm = function(){
                $scope.showLoginRegister = !$scope.showLoginRegister;
                $scope.showHomepage = !$scope.showHomepage;  
                $scope.showMentorRegistration = false;
                $scope.showforgotPassword = false;
            }
            
            $scope.forgotPassword = function(){
                $scope.showLoginRegister = false;
                $scope.showHomepage = false;  
                $scope.showMentorRegistration = false;
                $scope.showforgotPassword = true;
            }
            $scope.backToLoginpage = function(){
                $scope.showLoginRegister = true;
                $scope.showHomepage = false;  
                $scope.showMentorRegistration = false;
                $scope.showforgotPassword = false;
                $scope.showMentorMenteeSignIn = true;
            }
            
            
            $scope.mentorRegistration = function(){
                $scope.showMenteeregistration = false;
                $scope.showMentorMenteeSignIn = false;
                $scope.showMentorRegistration = true;
                $scope.showforgotPassword = false;
            }
            
            $scope.backToMentorSignin = function(){
                $scope.showMenteeregistration = false;
                $scope.showMentorMenteeSignIn = true;
                $scope.showMentorRegistration = false;
                $scope.showforgotPassword = false;
            }
            
            $scope.registerMentor = function (){
                if($scope.MentorPswrdToVerify.length < 7 || $scope.MentorPswrdToVerify == undefined) {
                    Swal.fire({
                        icon: 'error',
                        title: '',
                        text: 'Password length should not be less then 8 characters',
                    })
                    return;
                }
                if($scope.MentorPswrdToVerify == undefined || $scope.MentorNameToVerify == undefined) {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Please enter valid loginId',
                    })
                    return;
                }
                
                $scope.showSpinner = true;
                $scope.MentorPswrdToVerify = $scope.MentorPswrdToVerify.replaceAll(/\s+/g,'');
                MSigninHelper.mentorRegistration($scope.MentorPswrdToVerify,$scope.MentorNameToVerify, function(result, event) {
                    if(event.status) {
                        if(result != null || result != undefined){
                            if(result.length == 23){
                                /*   Swal.fire(
                                    '',
                                    'Temporary URL sent successfully to your registered email!',
                                    'success'
                                )
                                 $scope.showLoginRegister = false;
                                $scope.showHomepage = true;  
                                $scope.showMentorRegistration = false;
                                $scope.showforgotPassword = false;
                                location.reload();  */
                                let mainURL = window.location.origin+'/mentorship';
                                window.location.replace(mainURL + "/"+"MentorRegistrationForm?id="+result);
                            }
                            else if (result == 'regesterd'){
                                Swal.fire({
                                    icon: 'error',
                                    title: 'Oops...',
                                    text: 'You are already registered as Mentor Please login.',
                                })
                                location.reload();
                            }
                            
                                else{
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Oops...',
                                        text: 'Something went wrong please try again.',
                                    })
                                }
                            
                        }
                    }  else if(event.type==='exception'){
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }
                     $scope.showSpinner = false;
                     $scope.$apply();
                 });
                
                
            }
            
            $scope.mentorLogin = true;
            $scope.menteeLogin = false;
            
            $scope.resetPassword = function(){
                if($scope.verifyEmail == undefined) {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Please enter valid EmailId',
                    })
                    return;
                }
                $scope.showSpinner = true;
                MSigninHelper.passwordReset($scope.verifyEmail, function(result, event) {
                    if(event.status) {
                        if(result != null || result != undefined){
                            if(result == "Temporary URL sent successfully to your registered email."){
                                Swal.fire(
                                    '',
                                    'Temporary URL sent successfully to your registered email!',
                                    'success'
                                )
                                $scope.showLoginRegister = false;
                                $scope.showHomepage = true;  
                                $scope.showMentorRegistration = false;
                                $scope.showforgotPassword = false;
                            }else{
                                Swal.fire({
                                    icon: 'error',
                                    title: 'Oops...',
                                    text: 'Something went wrong please try again.',
                                })
                            }
                        }else if(result == null){
                            Swal.fire({
                                icon: 'error',
                                title: 'Oops...',
                                text: 'Something went wrong please try again.',
                            })
                        }
                    }
                    
                    else if(event.type==='exception'){
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Someting went Wrong Please refresh the page',
                        })
                    }
                    $scope.showSpinner = false;
                    $scope.$apply();
                });
            }
            $scope.forgotPassword = function(){
                $scope.showHomepage = false;  
                $scope.showMentorRegistration = false;
                $scope.showMentorMenteeSignIn = false;
                $scope.showforgotPassword = true;
                
            }
        });
